    "tunnel-restart": "./scripts/tunnel-restart.sh",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "node -r ./node-fix.js node_modules/.bin/expo start --clear --android",
    "ios": "node -r ./node-fix.js node_modules/.bin/expo start --clear --ios",
    "lint": "expo lint",
    "clean:metro": "watchman watch-del-all || true && rm -rf $TMPDIR/metro-* && rm -rf node_modules/.cache/metro"
  },
